<interface>
	<name>Get YAMAHA RT PP Interfaces</name>
	<description>Queries a YAMAHA RT for a PP list of monitorable interfaces</description>
	<oid_index>.1.3.6.1.4.1.1182.2.3.9.1.1</oid_index>
	<oid_num_indexes>.1.3.6.1.4.1.1182.2.3.1.0</oid_num_indexes>
	<index_order>yrIfPpDescr:yrIfPpIndex</index_order>
	<index_order_type>numeric</index_order_type>
	<index_title_format>|chosen_order_field|</index_title_format>

	<fields>
		<yrIfPpIndex>
			<name>Index</name>
			<method>walk</method>
			<source>value</source>
			<direction>input</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.1</oid>
		</yrIfPpIndex>
		<yrIfPpDescr>
			<name>Description</name>
			<method>walk</method>
			<source>value</source>
			<direction>input</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.2</oid>
		</yrIfPpDescr>
		<yrIfPpType>
			<name>Type</name>
			<method>walk</method>
			<output_format>ascii</output_format>
			<source>value</source>
			<direction>input</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.3</oid>
		</yrIfPpType>
		<yrIfPpSpeed>
			<name>Speed</name>
			<method>walk</method>
			<source>value</source>
			<direction>input</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.5</oid>
		</yrIfPpSpeed>
		<yrIfPpAdminStatus>
			<name>AdminStatus</name>
			<method>walk</method>
			<source>VALUE/REGEXP:[a-zA-Z]{1,}\(([1-]{1})+\)$</source>
			<direction>input-output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.7</oid>
		</yrIfPpAdminStatus>
		<yrIfPpOperStatus>
			<name>Status</name>
			<method>walk</method>
			<source>VALUE/REGEXP:[a-zA-Z]{1,}\(([1-]{1})+\)$</source>
			<direction>input-output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.8</oid>
		</yrIfPpOperStatus>
		<yrIfPpInOctets>
			<name>Bytes In</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.10</oid>
		</yrIfPpInOctets>
		<yrIfPpOutOctets>
			<name>Bytes Out</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.16</oid>
		</yrIfPpOutOctets>
		<yrIfPpInDiscards>
			<name>Discarded Packets In</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.13</oid>
		</yrIfPpInDiscards>
		<yrIfPpOutDiscards>
			<name>Discarded Packets Out</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.19</oid>
		</yrIfPpOutDiscards>
		<yrIfPpInNUcastPkts>
			<name>Non-Unicast Packets In</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.12</oid>
		</yrIfPpInNUcastPkts>
		<yrIfPpOutNUcastPkts>
			<name>Non-Unicast Packets Out</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.18</oid>
		</yrIfPpOutNUcastPkts>
		<yrIfPpOutUcastPkts>
			<name>Unicast Packets In</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.11</oid>
		</yrIfPpOutUcastPkts>
		<yrIfPpOutNUcastPkts>
			<name>Unicast Packets Out</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.17</oid>
		</yrIfPpOutNUcastPkts>
		<yrIfPpInErrors>
			<name>Errors In</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.14</oid>
		</yrIfPpInErrors>
		<yrIfPpOutErrors>
			<name>Errors Out</name>
			<method>walk</method>
			<source>value</source>
			<direction>output</direction>
			<oid>.1.3.6.1.4.1.1182.2.3.9.1.20</oid>
		</yrIfPpOutErrors>
	</fields>
</interface>
